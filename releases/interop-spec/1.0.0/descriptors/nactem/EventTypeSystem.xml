      <supertypeName>uk.ac.nactem.uima.cas.semantic.Event</supertypeName>
      <features>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.semantic.NamedEventParticipant</name>
      <description>EventParticipant with a name/category.</description>
      <supertypeName>uk.ac.nactem.uima.cas.semantic.EventParticipant</supertypeName>
      <features>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.semantic.NamedAnnotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.semantic.EventMetaknowledge</name>
