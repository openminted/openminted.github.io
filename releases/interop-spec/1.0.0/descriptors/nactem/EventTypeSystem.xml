<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>EventTypeSystem</name>
  <description/>
  <version>0.6</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.semantic.Event</name>
      <description>A span-of-text annotation that contains a list of arguments.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>participants</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uk.ac.nactem.uima.cas.semantic.EventParticipant</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.semantic.EventParticipant</name>
      <description>A participant of an Event</description>
      <supertypeName>uima.cas.AnnotationBase</supertypeName>
      <features>
        <featureDescription>
          <name>target</name>
          <description>Participant's target annotation.</description>
          <rangeTypeName>uima.cas.AnnotationBase</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>uk.ac.nactem.uima.cas.semantic.NamedEvent</name>
