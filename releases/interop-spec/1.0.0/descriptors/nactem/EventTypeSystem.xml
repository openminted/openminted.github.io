      <features>
        <featureDescription>
          <name>event</name>
          <description/>
          <rangeTypeName>uk.ac.nactem.uima.cas.semantic.Event</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>value</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
